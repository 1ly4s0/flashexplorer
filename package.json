{
  "name": "flashexplorer",
  "productname": "Flash Explorer",
  "version": "1.0.1",
  "description": "A desktop file explorer application built with ElectronJS, featuring basic file operations using robocopy for enhanced performance.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec npm start",
    "build": "node build.js"
  },
  "nodemonConfig": {
    "ext": "js,html,css"
  },
  "keywords": [
    "ElectronJS",
    "File Explorer",
    "Desktop Application",
    "File Management",
    "File Navigation",
    "Windows Explorer",
    "File Operations",
    "Copy Files",
    "Paste Files",
    "Delete Files",
    "File System",
    "FontAwesome Icons",
    "HTML",
    "CSS",
    "Node.js",
    "JavaScript",
    "Cross-Platform",
    "Robocopy",
    "User Interface",
    "Breadcrumb Navigation"
  ],
  "author": "1ly4s0",
  "license": "MIT",
  "devDependencies": {
    "electron": "^31.2.1",
    "electron-builder": "^24.13.3",
    "nodemon": "^3.1.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "path": "^0.12.7"
  }
}
